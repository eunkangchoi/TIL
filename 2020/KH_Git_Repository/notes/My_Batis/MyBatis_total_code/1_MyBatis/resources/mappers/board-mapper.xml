<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="boardMapper">
	<!--  숫자값만 반환하면된다. 즉  -->
  <select id="selectListCount" resultType="_int">
  	SELECT COUNT(*)
  	FROM BOARD
  	WHERE B_STATUS='Y'
  </select>
</mapper>
